	/* IMPORT SECTION */

	@import url("http://fonts.googleapis.com/css?family=Raleway:200,300,600,700&subset=latin,latin-ext");



	/* BASIC STYLES */

	html, body {
		height: 100%;
	}

	body {
		font-family: "Raleway", Helvetica, Arial, sans-serif;
		font-size: 14px;
		font-weight: 200;
		line-height: 1.7857;
		letter-spacing: 0.075em;
		color: #333333;
		background-color: #fff;
	}

	a { 
		color: #000;
		-webkit-transition: all 0.2s ease-in-out;
		-moz-transition: all 0.2s ease-in-out;
		-ms-transition: all 0.2s ease-in-out;
		-o-transition: all 0.2s ease-in-out;
		transition: all 0.2s ease-in-out;
	}

	a:hover {
		color: #000;
	}

	a, a:hover, a:active, a:focus, button:focus {
		outline: 0;
		border: 0;
		text-decoration: none;
	}

	#st-preloader {
		background: #FFF;
		bottom: 0;
		left: 0;
		position: fixed;
		right: 0;
		top: 0;
		z-index: 9999;
	}

	p {
		line-height: 1.5;
	}

	h2, h4{
		font-family: 'Raleway', sans-serif;
		font-weight: 700;
		text-transform: uppercase;
		text-align: center;
	}

	h1, h3, h5, h6{
		font-family: 'Raleway', sans-serif;
		font-weight: 700;
		text-transform: uppercase;
	}

	input, textarea{
		outline: none;
	}

	label{
		font-weight: 300;
	}

	#pre-status,
	.preload-placeholder {
		background-image: url('<%= asset_path("preloader.gif") %>');
		background-position: center;
		background-repeat: no-repeat;
		height: 40px;
		left: 50%;
		margin: -20px 0 0 -20px;
		position: absolute;
		top: 50%;
		width: 40px;
	}

	.preload-placeholder {
		background: none;
		left: 0;
		margin: 0;
		text-align: center;
		top: 65%;
	}

	.st-border{
		width: 70%;
		height: 1px;
		display: block;
		background-color: #000;
		margin-left: auto;
		margin-right: auto;
		margin-top: 20px;
		margin-bottom: 20px;
	}

	.overlay{
		background-color: #000;
		position: absolute;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
		opacity: 0.8;
	}


	#h {
		background: url('<%= asset_path("fruits-bg.jpg") %>') no-repeat center center;
		background-size: 100% ;
		background-color: #fff;
		padding-top: 20px;
		text-align: center;
		background-attachment: relative;
		background-position: center center;
		width: 100%;
		height: 80%;
		color: #21437E;
	}

	#order {
		width: 70%;
		padding-top: 30px;
		padding-bottom: 30px;
		margin-left: auto;
		margin-right: auto;
		border: 2px solid #e9e9e9;
		border-radius: 10px;
	}

	#details {
		width: 70%;
		padding-top: 10px;
		padding-bottom: 30px;
		margin-left: auto;
		margin-right: auto;
		border: 2px solid #e9e9e9;
		border-radius: 10px;
	}

	#margin{
		margin-top: 20px;
		margin-bottom: 20px;
	}

	.col-centered{
		float: none;
		margin: 0 auto;
	}

	.col-botbordered{
		border-bottom: 1px solid #ccc;
	}

	.col-bordered{
		border-right: 1px solid #ccc;
		margin-right: 30px;
	}

	/* BUTTONS */

	.btn-send{
		background-color: #383838;
		border-radius: 10;
		border: 0;
		color: #fff;
		padding-left: 25px;
		padding-right: 25px;
		padding-top: 7px;
		padding-bottom: 7px;
		-webkit-transition: all 0.2s ease-in-out;
		-moz-transition: all 0.2s ease-in-out;
		-ms-transition: all 0.2s ease-in-out;
		-o-transition: all 0.2s ease-in-out;
		transition: all 0.2s ease-in-out;
		margin-bottom: 20px;
	}

	.btn-send:hover{
		background-color: #000;
		color: #fff;
	}

	.btn-conf{
		background-color: #FFD333;
		border-radius: 10;
		border: 0;
		color: #383838;
		padding-left: 25px;
		padding-right: 25px;
		padding-top: 7px;
		padding-bottom: 7px;
		-webkit-transition: all 0.2s ease-in-out;
		-moz-transition: all 0.2s ease-in-out;
		-ms-transition: all 0.2s ease-in-out;
		-o-transition: all 0.2s ease-in-out;
		transition: all 0.2s ease-in-out;
	}

	.btn-conf:hover{
		background-color: #e8bf2c;
		color: #383838;
	}

	.btn-readmore{
		background-color: #383838;
		border-radius: 0;
		border: 0;
		color: #fff;
		padding: 10px 20px;
		margin-top: 20px;
		text-transform: uppercase;
		-webkit-transition: all 0.2s ease-in-out;
		-moz-transition: all 0.2s ease-in-out;
		-ms-transition: all 0.2s ease-in-out;
		-o-transition: all 0.2s ease-in-out;
		transition: all 0.2s ease-in-out;
	}

	.btn-readmore:hover{
		background-color: #000;
		color: #fff;	
	}

	.line-separator{
		height:1px;
		background:#717171;
		border-bottom:1px solid #313030;
		display: inline;
	}


	/* HEADER */

	#header{}

	#header .navbar {
		min-height: 80px;
		margin-bottom: 0;
		border-radius: 0;
	}

	.st-navbar{
		background-color: #ffffff;
		border-bottom: 1px solid #f0f0f0;
	}

	.st-navbar ul{
		margin-top: 15px;
	}

	.st-navbar ul li a{
		text-transform: uppercase;
		font-weight: 600;
		padding: 0;
		margin: 14px;
	}

	.st-navbar ul li a:hover,
	.st-navbar ul li a:focus {
		background-color: transparent;
		color: #000;
	}

	.st-navbar .navbar-header{
		margin-top: 25px;
	}

	.st-navbar .navbar-toggle .icon-bar {
		background-color: #000;
	}

	.st-navbar .navbar-toggle{
		margin-top: 7px;
		margin-right: 25px;
		padding: 0;
		border-radius: 0;
	}




	/* FEATURE */
	.box {
		padding-top: 15px;
		padding-bottom: 15px;
		background: #fff;
		border: 1px solid #e9e9e9;
		position: relative;
	}
	.box.first {
		margin-top: -5%;
		border-radius: 5px 5px 5px 5px;
		width: 55%;
		height: 16%;
		margin-right: auto;
		margin-left: auto;
		margin-bottom: -120px;
	}
	.box h6{
		text-align: center;
	}
	.box p{
		font-size: 10px;
		margin-top: -5px;
		text-align: center;
	}




	/* PRODUCTS */

	#products{
		padding-top: 130px;
		padding-bottom: 120px;
	}

	#orders{
		padding-top: 130px;
		padding-bottom: 120px;
	}

	.product-margin{
		margin-top: 30px;
	}

	.product-margin:hover{
		box-shadow: 0px 0px 18px #ddd;
	}

	.product-image{
		position: relative;
		padding-top: 10px;
	}

	.product-info{
		padding: 10px;
	}

	.product-info h4{
		margin-top: 0;
		margin-bottom: 6px;
	}

	.product-margin .product-add{
		position: absolute;
		width: 100%;
		height: 100%;
		left: 0;
		top: 0;
		text-align: center;
		opacity: 0;
		-webkit-transition: all 0.3s ease-out 0s;
		-moz-transition: all 0.3s ease-out 0s;
		-ms-transition: all 0.3s ease-out 0s;
		-o-transition: all 0.3s ease-out 0s;
		transition: all 0.3s ease-out 0s;
		-moz-transform: scale(0.5);
		-webkit-transform: scale(0.5);
		-o-transform: scale(0.5);
		-ms-transform: scale(0.5);
		transform: scale(0.5);
	}

	.product-margin:hover .product-add{
		opacity: 1;
		-moz-transform: scale(1);
		-webkit-transform: scale(1);
		-o-transform: scale(1);
		-ms-transform: scale(1);
		transform: scale(1);
	}

	.product-margin .product-add a{
		position: relative;
		font-size: 25px;
		color: #fff;
		top: 40%;
	}

	.product-margin:hover .product-add a{
		margin: 6px;
	}



	/* CONTACT */

	#contact{
		padding-top: 130px;
		padding-bottom: 120px;
	}
	
	.contact-info .contact-content{
		margin-bottom: 30px;
	}

	.contact-info p{
		margin-bottom: 20px;
	}

	.contact-info p i{
		font-size: 16px;
		margin-right: 10px;
	}

	.contact-form{}

	.contact-form input[type="text"],
	.contact-form input[type="email"], 
	.contact-form input[type="password"]{
		border: 1px solid #E8E8E8;
		padding: 8px 14px;
		width: 100%;
		margin-bottom: 20px;
	}

	.contact-form textarea{
		border: 1px solid #E8E8E8;
		padding: 8px 14px;
		width: 100%;
		margin-bottom: 20px;
	}




	/* FOOTER */

	#footer{
		background-color: #262626;
		padding-top: 25px;
		padding-bottom: 20px;
		color: rgba(255, 255, 255, 0.4);
	}

	#footer a{
		color: rgba(255, 255, 255, 0.4);
	}
	#footer a:hover{
		color: #fff;
	}

	.footer-social-icons{
		text-align: right;
	}

	.footer-social-icons a{
		margin: 0 5px;
		font-size: 16px;
	}

	.footer-social-icons span{
		margin-right: 10px;
	}

	




